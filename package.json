{
  "name": "brady",
  "version": "1.0.0",
  "description": "Bradyinc.com uses [Hugo](https://gohugo.io/) to build a static website. The page content is stored in [Markdown](https://github.com/adam-p/markdown-here/wiki/Markdown-Cheatsheet) templates.",
  "main": "index.js",
  "scripts": {
    "sass:build": "node-sass --output-style=compressed --source-map=true themes/brady/static/scss/ -o themes/brady/static/css/",
    "publish:build": "npm run sass:build && hugo --destination /dist/public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaylah/brady.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kaylah/brady/issues"
  },
  "homepage": "https://github.com/kaylah/brady#readme",
  "devDependencies": {
    "node-sass": "^4.7.2"
  }
}
